<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
    <rule>  
		<note>action访问重写</note>
        <from>/(.*)/(.*).html?(.*)</from>
        <to>/$1!$2.ac?$3</to>  
    </rule> 
    <rule>  
		<note>css地址重写</note>
        <from>/req/css/(.*)</from>
        <to>/css/$1</to>  
    </rule> 
    <rule>  
		<note>img地址重写</note>
        <from>/req/img/(.*)</from>
        <to>/img/$1</to>  
    </rule>
</urlrewrite>

